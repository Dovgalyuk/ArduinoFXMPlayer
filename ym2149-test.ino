struct RAMItem
{
  uint16 addr;
  uint8 val;
};

RAMItem ram[256];
uint8 ramsize;

void setup()
{
  set_ym_clock();
  set_bus_ctl();

  // reset registers
  //for (int i=0; i<16; i++) {
  //  send_data(i, 0);
  //}
}

uint8 peek(uint16 addr)
{
    uint8 res = 0;
    if (addr >= ROM_ADDR && addr < ROM_ADDR + sizeof(rom))
    {
      res = pgm_read_byte(rom + addr - ROM_ADDR);
    }
    for (int i = 0 ; i < ramsize ; ++i)
      if (ram[i].addr == addr)
      {
        res = ram[i].val;
        break;
      }
    return res;
}

void poke(uint16 addr, uint8 c)
{
  for (int i = 0 ; i < ramsize ; ++i)
    if (ram[i].addr == addr)
    {
      ram[i].val = c;
      return;
    }
  ram[ramsize].addr = addr;
  ram[ramsize].val = c;
  ++ramsize;
}

void handle_output(uint a, uint b)
{
    /*
OUT (0xfffd)   - Select a register 0-14
IN  (0xfffd)   - Read the value of the selected register
OUT (0xbffd)   - Write to the selected register
    */
    static uint8 reg;
    if (a == 0xfffd)
      reg = b;
    else if (a == 0xbffd)
      send_data(reg, b);
}

uint handle_input(uint a)
{
  uint8 b = 0xff;
//    printf("IN %x\n", a);
  return b;
}

void loop()
{
  Z80 z80;
  z80.registers.pc = 0;
  z80.registers.sp = 0;

  for/*ever*/(;;) {
    if (z80.halt)
    {
      z80.halt = 0;
      ++z80.registers.pc;
    }
    //_delay_ms(20.);
    z80.run(1000000);
  }
}

